{
  "name": "@repo/ui",
  "version": "0.0.0",
  "private": true,
  "exports": {
    "./*": "./src/*.tsx"
  },
  "scripts": {
    "lint": "eslint . --max-warnings 0",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx}\"",
    "generate:component": "turbo gen react-component",
    "check-types": "tsc --noEmit"
  },
  "devDependencies": {
    "@tools/eslint": "workspace:*",
    "@tools/prettier": "workspace:*",
    "@tools/typescript": "workspace:*",
    "@types/node": "^22.15.3",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2",
    "eslint": "^9.39.1",
    "prettier": "^3.7.4",
    "typescript": "5.9.2"
  },
  "prettier": "@tools/prettier/react",
  "dependencies": {
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  }
}